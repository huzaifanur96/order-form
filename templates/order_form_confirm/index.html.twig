                <!-- funeral home -->
                <div id="funeral-home">
                    <div class="flex flex-wrap">
                        <div class="mt-6 w-full md:w-1/3 px-3">
                            <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" for="funeral-home-name">
                                Funeral Home
                            </label>
                            <p class="py-3 px-4 bg-gray-100">{{ funeral_home_name }}</p>
                        </div>
                        <!-- Contact -->
                        <div class="mt-6 w-full md:w-1/3 px-3">
                            <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" for="funeral-home-contact">
                                Contact
                            </label>
                            <p class="py-3 px-4 bg-gray-100">{{ funeral_home_contact }}</p>
