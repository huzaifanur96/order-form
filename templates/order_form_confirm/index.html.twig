{% extends 'base.html.twig' %}

{% block title %}
  Order
{% endblock %}

{% block body %}
  <div class="container mx-auto">
        <!-- BETA tag -->
        <div class="mt-2 mb-8 text-right">
            <small style="background-color:#f8fafc; opacity: 72%;" class="border-2 rounded-xl border-gray-200 text-xs text-gray-700 px-3">Beta</small>
        </div>
        <h1 class="text-5xl mt-6 text-center font-semibold text-gray-400">Honoring A Life</h1>
        <h2 class="text-3xl text-center text-gray-400 ">Order Form</h2>
        <h4 class="text-2xl text-center">Confirmation</h4>
        <p class="text-xl text-center">Please confirm the details and submit the form</p>
        <!-- separator -->
        <hr class="mb-4 mt-4 mx-3" />

        <!-- form response -->
        <div class="mt-12">
            <!-- Priliminary information -->
